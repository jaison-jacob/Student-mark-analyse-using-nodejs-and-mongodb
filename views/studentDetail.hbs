<!DOCTYPE html>
<html>

<head>

<title>student data</title>

</head>
<style>
    .GFG{ 
            background-color:bisque; 
            border: 2px solid rgba(17, 3, 99, 0.068); 
            color: green; 
            padding: 5px 10px; 
            text-align: center; 
            display: inline-block; 
            font-size: 10px; 
            margin: 10px 10px; 
            cursor: pointer; 
          
        }
</style>
<body>

<h3>STUDENT DATA TABLE</h3>

<table border="1px" border-color="red"> 

<thead>
    <tr>

        <th>
            ID
        </th>
        <th>
            NAME
        </th>
        <th>
            TAMIL
        </th>
        <th>
            ENGLISH
        </th>
        <th>
            MATHS
        </th>
        <th>
            SCIENCE
        </th>
        <th>
            SOCIAL
        </th>
         <th>
            
        </th>
        <th>

        </th>
    </tr>
</thead>
<tbody>
{{#each list}}

<tr>
<td>{{this.id}}</td>
<td>{{this.name}}</td>
<td>{{this.tamil}}</td>
<td>{{this.english}}</td>
<td>{{this.maths}}</td>
<td>{{this.science}}</td>
<td>{{this.social}}</td>
<td><button id="btn2" 
    onclick="window.location.href = 'http://localhost:8080/result/{{this._id}}';"> 
        GET RESULT
    </button></td>
    <td><button id="btn2" 
    onclick="window.location.href = 'http://localhost:8080/adminpage';"> 
        back
    </button></td>
</tr>

{{/each}}

</tbody>

</table>


</body>

</html>








